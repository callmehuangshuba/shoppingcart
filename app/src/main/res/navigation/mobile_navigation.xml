<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.android.LitangPrince.ui.home.HomeFragment"
        android:label="首页"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_snack"
        android:name="com.android.LitangPrince.ui.smoking.SmokingFragment"
        android:label="商店"
        tools:layout="@layout/fragment_snack" />

    <fragment
        android:id="@+id/navigation_place"
        android:name="com.android.LitangPrince.ui.place.PlaceFragment"
        android:label="购物车"
        tools:layout="@layout/fragment_place" />

    <fragment
        android:id="@+id/navigation_my"
        android:name="com.android.LitangPrince.ui.my.MyFragment"
        android:label="我的"
        tools:layout="@layout/fragment_my" />
</navigation>